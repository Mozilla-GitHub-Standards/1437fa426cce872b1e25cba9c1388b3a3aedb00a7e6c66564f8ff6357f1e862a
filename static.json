{
    "root": "build/",
    "https_only": true,
    "headers": {
        "/**": {
            "Strict-Transport-Security": "max-age=7776000"
        },
        "/__heartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        },
        "/__lbheartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        }
    },
    "routes": {
        "/": "index.html",
        "/dashboard/user-activity": "index.html",
        "/dashboard/usage-behavior": "index.html",
        "/dashboard/hardware": "index.html",
        "/__heartbeat__": "/ops/heartbeat.html",
        "/__lbheartbeat__": "/ops/lbheartbeat.html",
        "/__version__": "/ops/version.json"
    },
    "error_page": "index.html"
}
