{
    "root": "build/",
    "https_only": true,
    "headers": {
        "/__heartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        },
        "/__lbheartbeat__": {
            "Cache-Control": "no-cache, no-store, must-revalidate",
            "Pragma": "no-cache",
            "Expires": 0
        },
        "/**": {
            "Strict-Transport-Security": "max-age=7776000"
        }
    },
    "routes": {
        "/__heartbeat__": "/ops/heartbeat.html",
        "/__lbheartbeat__": "/ops/lbheartbeat.html",
        "/__version__": "/ops/version.json",
        "/**": "index.html"
    }
}
