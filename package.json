{
    "name": "ensemble",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "npm-run-all --parallel watch:css start:app",
        "start:app": "react-scripts start",
        "build": "npm-run-all build:css build:app",
        "build:app": "react-scripts build",
        "build:css": "stylus --sourcemap ./src/components/views/styl/* --out ./src/components/views/css/",
        "build:version.json": "node scripts/generateVersionJSON",
        "eject": "react-scripts eject",
        "validate": "npm run test",
        "test": "npm-run-all lint test:jest test:nightwatch:local",
        "test:jest": "CI=true react-scripts test",
        "test:nightwatch:local": "nightwatch --env default",
        "test:nightwatch:stage": "NIGHTWATCH_LAUNCH_URL='https://data-ensemble.stage.mozaws.net' NIGHTWATCH_SITE_TITLE='Firefox Public Data Report (Stage)' nightwatch --env default",
        "test:nightwatch:prod": "NIGHTWATCH_LAUNCH_URL='https://data.firefox.com' nightwatch --env default",
        "test:nightwatch:browserstack:local": "RUN_ON_BSTACK=true BSTACK_LOCAL=true NIGHTWATCH_TIMEOUT=25000 npm-run-all --sequential 'test:nightwatch:browserstack:env:*'",
        "test:nightwatch:browserstack:stage": "RUN_ON_BSTACK=true NIGHTWATCH_LAUNCH_URL='https://data-ensemble.stage.mozaws.net' NIGHTWATCH_SITE_TITLE='Firefox Public Data Report (Stage)' NIGHTWATCH_TIMEOUT=25000 npm-run-all --sequential 'test:nightwatch:browserstack:env:*'",
        "test:nightwatch:browserstack:prod": "RUN_ON_BSTACK=true NIGHTWATCH_LAUNCH_URL='https://data.firefox.com' NIGHTWATCH_TIMEOUT=25000 npm-run-all --sequential 'test:nightwatch:browserstack:env:*'",
        "test:nightwatch:browserstack:env:ie": "nightwatch --env 'BrowserStack:ie'",
        "test:nightwatch:browserstack:env:edge": "nightwatch --env 'BrowserStack:edge'",
        "test:nightwatch:browserstack:env:firefoxWin7": "nightwatch --env 'BrowserStack:firefoxWin7'",
        "test:nightwatch:browserstack:env:firefoxSierra": "nightwatch --env 'BrowserStack:firefoxSierra'",
        "test:nightwatch:browserstack:env:chromeWin7": "nightwatch --env 'BrowserStack:chromeWin7'",
        "test:nightwatch:browserstack:env:chromeSierra": "nightwatch --env 'BrowserStack:chromeSierra'",
        "test:nightwatch:browserstack:env:jsDisabled": "nightwatch --env jsDisabled",
        "lint": "npm-run-all lint:*",
        "lint:js-extra": "npx eslint --config .eslintrc.extra.js --ext .js --ext .json .",
        "lint:styl": "stylint src/components/views/styl",
        "posttest": "npm audit || true",
        "size": "source-map-explorer build/static/js/main.*",
        "watch:css": "stylus --watch --sourcemap src/components/views/styl/* --out ./src/components/views/css/"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "d3-scale": "2.2.2",
        "d3-selection": "1.4.0",
        "d3-shape": "1.3.4",
        "d3-transition": "1.2.0",
        "dateformat": "3.0.3",
        "distinct-colors": "1.0.4",
        "markdown-it": "8.4.2",
        "markdown-it-sup": "1.0.0",
        "memoize-one": "5.0.0",
        "metrics-graphics": "2.15.6",
        "npm-run-all": "4.1.5",
        "react": "16.8.3",
        "react-app-polyfill": "0.2.0",
        "react-dom": "16.8.3",
        "react-ga": "2.5.7",
        "react-helmet": "5.2.0",
        "react-loadable": "5.5.0",
        "react-metrics-graphics": "1.0.6",
        "react-refetch": "2.0.3",
        "react-router-dom": "4.3.1",
        "react-scripts": "2.1.5",
        "react-spinners": "0.5.1",
        "stylus": "0.54.5"
    },
    "devDependencies": {
        "browserstack-automate": "1.0.2",
        "chromedriver": "2.46.0",
        "enzyme": "3.9.0",
        "enzyme-adapter-react-16": "1.9.1",
        "eslint-plugin-jest": "22.3.0",
        "eslint-plugin-json": "1.4.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-react": "7.12.4",
        "nightwatch": "1.0.19",
        "react-test-renderer": "16.8.3",
        "request": "2.88.0",
        "source-map-explorer": "1.7.0",
        "stylint": "1.5.9"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 10",
        "not op_mini all"
    ]
}
